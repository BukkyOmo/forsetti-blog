{
  "name": "forsetti-blog",
  "version": "1.0.0",
  "description": "a blog",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha --exit --require @babel/register --timeout 60000 ./server/test/**/*.js",
    "start": "npm run build && node server/index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node server/index.js",
    "build": "babel server --out-dir dist",
    "serve": "node dist/index.js",
    "clean": "rm -r dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BukkyOmo/forsetti-blog.git"
  },
  "author": "forsetti-team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BukkyOmo/forsetti-blog/issues"
  },
  "homepage": "https://github.com/BukkyOmo/forsetti-blog#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "express": "^4.16.4",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "jshint": "^2.10.2",
    "nodemon": "^1.18.10"
  }
}
